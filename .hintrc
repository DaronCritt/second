{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/text-alternatives": "off",
    "disown-opener": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-decoration-thickness",
          "text-underline-offset"
        ]
      }
    ]
  }
}